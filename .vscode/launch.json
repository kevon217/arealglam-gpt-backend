{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/app"
      }
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "test_assistants_pipeline",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/../../app"
      }
    }
  ]
}
